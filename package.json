{
  "name": "dosn",
  "version": "0.0.8",
  "description": "nginx+node+koa+socket+mysql+ejs 简单尝试",
  "scripts": {
    "start": "node bin/www",
    "tsc": "tsc",
    "server": "node --trace-deprecation server.js",
    "wechat": "node --trace-deprecation wechat.js",
    "chat:http": "set port=3030 && node --trace-deprecation chat.js",
    "chat:linux": "export port=3030 && node --trace-deprecation chat.js",
    "test": "tsc && node ./src/test.js",
    "watch": "ts-node ./src/test.ts",
    "watch-serve": "nodemon ./src/test.ts"
  },
  "dependencies": {
    "ejs": "^2.6.1",
    "http-proxy-middleware": "~0.19.1",
    "koa": "^2.7.0",
    "koa-bodyparser": "^4.2.1",
    "koa-multer": "^1.0.2",
    "koa-passport": "^4.1.3",
    "koa-router": "^7.4.0",
    "koa-session": "^5.12.3",
    "koa-static": "^5.0.0",
    "koa-static-router": "^1.3.0",
    "koa-views": "^6.2.0",
    "log4js": "^4.5.1",
    "mysql": "^2.17.1",
    "redis": "^2.8.0",
    "reflect-metadata": "^0.1.13",
    "routing-controllers": "^0.7.7",
    "socket.io": "^2.2.0",
    "typescript": "~3.5.3"
  },
  "devDependencies": {
    "@types/koa": "^2.0.50",
    "@types/koa-bodyparser": "^4.3.0",
    "@types/koa-router": "^7.0.42",
    "@types/node": "^12.7.8",
    "nodemon": "^1.19.1",
    "ts-node": "^8.4.1"
  },
  "keywords": [
    "node",
    "ejs",
    "mysql",
    "nginx",
    "screen",
    "koa",
    "koa2",
    "socket.io"
  ],
  "repository": {
    "type": "git",
    "url": "git@github.com:manydots/node.git"
  },
  "homepage": "https://github.com/manydots/node#readme",
  "author": "mo.chen",
  "license": "MIT"
}